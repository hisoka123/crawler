/*apitest*/
.sr-zql-container {
	margin-top: 80px;
	width: 80%;
}

.sr-zql-rowbox {
	min-height:490px; 
	border-radius:15px;
	box-shadow:0px 0px 15px #D6D6D6;
	-moz-box-shadow:0px 0px 15px #D6D6D6; 
	-webkit-box-shadow:0px 0px 15px #D6D6D6; 
}

.sr-zql-boxleft {
	background-color: #F7F7F7; 
	min-height:490px;
	border-radius:15px 0 0 15px;
	padding:30px 15px;
}

.sr-zql-boxleft form>div {
	margin-bottom: 15px;
	font-size: 12px;
}

.sr-zql-boxleft form select {
	outline: none;
	height: 25px;
	line-height: 25px;
	width: 140px;
	border: 1px solid #D3D3D3;
	border-radius: 3px;
	display: inline-block;
	text-indent: 2px;
	/* -webkit-appearance: none; 不加select下三角号*/
}

.sr-zql-boxleft form select option:nth-child(even) {
	background-color:#F4F4F4;
}

.sr-zql-boxleft form label {
	font-weight: normal;
}

.sr-zql-boxleft form .radio {
	text-align: center;
}

.sr-zql-boxleft form .radio label {
	margin-right: 15px;
}

.sr-zql-boxleft form hr {
	margin:7px 0;
	
}

.sr-zql-boxleft form>div.params {
	background-color: #FFFFFF;
	padding:7px 10px;
	border-radius:5px;
	border: 1px solid #E6E6E6;
	font-size: 12px;
}

.sr-zql-boxleft form>div.params input[type='text'] {
	border:1px solid #D3D3D3;
	border-radius:3px;
	text-indent: 5px;
}

.sr-zql-boxleft form>div.params button {
	outline: none;
	margin-top: 7px;
}

.sr-zql-boxleft form .param-container .param-key {
	width:70px;height:25px;
}

.sr-zql-boxleft form .param-container .param-value {
	width:95px;height:25px;
}

.sr-zql-boxleft form .param-container i:hover {
	cursor: pointer;
}

.sr-zql-boxright {
	min-height:490px;
	border-radius:0 15px 15px 0;
	padding:30px 40px;
	font-size: 12px;
}

.sr-zql-boxright-req {
	margin-bottom: 30px;
}

.sr-zql-boxright a {
	text-decoration: none;
}

.sr-zql-boxright a:hover {
	cursor:pointer;
	color:#337ab7;
	font-weight:bold;	
}

.sr-zql-boxright .sr-zql-boxright_cont {
	border: 1px solid lightgray;
	border-radius: 3px;
	margin-top: 7px;
	padding: 5px;
	color: #555555;
	word-wrap: break-word;
  	word-break: break-all;
}

.boxright-req {
	min-height: 100px;
}

.boxright-res {
	min-height: 240px;
	margin-bottom: 10px;
}


/*storm-ui*/
.sr-zql-sui-container {
	margin-top: 70px;
	margin-bottom: 20px;
	min-height:400px;
	height:expression(this.scrollHeight > 400 ? "auto":"400px");
}

.sr-zql-page-header {
	color:#0069d6;
	margin:5px 0 20px;
}

.sr-zal-topo-action-container {
	margin-bottom: 20px;
}
