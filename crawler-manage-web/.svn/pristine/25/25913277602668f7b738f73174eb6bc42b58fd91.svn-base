/*home页jumbotron附加样式*/
.sr-zql-jumbotron-margintop {
	margin-top:50px;
}

/*顶部导航栏样式重定义*/
.sr-zql-navbar-inverse {
	background-color:#185F7D;
	border-color:#185F7D;
}

.sr-zql-navbar-brand {
	display: inline-block;
	vertical-align: middle;
	padding-left: 50px;
	color: #fff;
}

a.sr-zql-navbar-brand:hover, a.sr-zql-navbar-brand:focus {
	color:#fff;
}

/*顶部右侧注册登录样式*/

.sr-zql-navbar-right {
    display: inline-block;
    vertical-align: middle;
    padding-left: 50px;
	padding-top: 15px;
    color: #fff;
}

a.sr-zql-navbar-right:hover, a.sr-zql-navbar-right:focus {
    color:#fff;
}

/*wiki*/
/*内容样式*/
.sr-zql-content {
	margin-top: 85px;
	margin-bottom: 20px;
	min-height:409px;
	height:expression(this.scrollHeight > 400 ? "auto":"400px");
}

/*左侧垂直导航栏*/
.sr-zql-main-nav {
	margin-left: 1px;
}

.sr-zql-main-nav.nav-tabs.nav-stacked>li>a {
	padding: 10px 8px;
	font-size: 12px;
	font-weight: 600;
	color: #4A515B;
	background: #E9E9E9;
	background: -moz-linear-gradient(top, #FAFAFA 0%, #E9E9E9 100%);
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%, #FAFAFA), color-stop(100%, #E9E9E9));
	background: -webkit-linear-gradient(top, #FAFAFA 0%, #E9E9E9 100%);
	background: -o-linear-gradient(top, #FAFAFA 0%, #E9E9E9 100%);
	background: -ms-linear-gradient(top, #FAFAFA 0%, #E9E9E9 100%);
	background: linear-gradient(top, #FAFAFA 0%, #E9E9E9 100%);
	filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#FAFAFA', endColorstr='#E9E9E9');
	-ms-filter: "progid:DXImageTransform.Microsoft.gradient(startColorstr='#FAFAFA', endColorstr='#E9E9E9')";
	border: 1px solid #D5D5D5;
	border-radius: 4px;
}

.sr-zql-main-nav.nav-tabs.nav-stacked>li>a>span {
	color: #4A515B;
}

.sr-zql-main-nav.nav-tabs.nav-stacked>li.active>a, .sr-zql-main-nav.nav-tabs.nav-stacked>li>a:hover {
	color: #FFF;
	background: #3C4049;
	background: -moz-linear-gradient(top, #4A515B 0%, #3C4049 100%);
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%, #4A515B), color-stop(100%, #3C4049));
	background: #185F7D/* -webkit-linear-gradient(top, #4A515B 0%, #3C4049 100%) */;
	background: -o-linear-gradient(top, #4A515B 0%, #3C4049 100%);
	background: -ms-linear-gradient(top, #185F7D 0%, #3C4049 100%);
	background: linear-gradient(top, #4A515B 0%, #3C4049 100%);
	filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#4A515B', endColorstr='#3C4049');
	-ms-filter: "progid:DXImageTransform.Microsoft.gradient(startColorstr='#4A515B', endColorstr='#3C4049')";
	border-color: #185F7D/* #2B2E33 */;
}

.sr-zql-main-nav.nav-tabs.nav-stacked>li.active>a, .sr-zql-main-nav.nav-tabs.nav-stacked>li>a:hover>span {
	color: #FFF;
}

.sr-zql-main-nav.nav-tabs.nav-stacked>li {
	margin-bottom: 4px;
}

.sr-zql-secondmenu a {
	font-size: 10px;
	color: #4A515B;
	text-align: left; /* center */
	text-indent: 12px;
}

/*右侧内容区域样式*/
.sr-zql-content-right {
	padding-left:30px;
	margin-right:30px;
	border-left:2px solid #F8F8F8;
	min-height:380px;
}

.sr-zql-content-right h1,h2 {
	margin:0;
}

pre.sr-zql-pre-without-border {
	border:0;
	background-color:#fff;
}

.sr-zql-content-right table thead tr {
	background-color:#F5F5F5;
}

.sr-zql-content-right-box {
	border: 1px solid lightgray;
	border-radius: 3px;
	padding: 20px;
	color: #555555;
	word-wrap: break-word;
	word-break: break-all;
}

/*附加功能：返回顶部*/
.sr-zql-totop {
	float:left;
	background-image:url("../imgs/icon/totop.png");
	background-repeat: no-repeat;
	background-position:0 0;	
	width: 48px;
	height:48px;
	cursor: pointer;
	z-index: 10000;
	position: fixed;
	right:30px;
	bottom:59px;
	display: none;
}

/*附加功能：返回顶部 和 返回*/
.sr-zql-topret-up {
	float:left;
	background-image:url("../imgs/icon/totop.png");
	background-repeat: no-repeat;
	background-position:0 0;	
	width: 48px;
	height:48px;
	cursor: pointer;
	z-index: 10000;
	position: fixed;
	right:30px;
	bottom:109px;
	display: none;
}
.sr-zql-topret-down {
	float:left;
	background-image:url("../imgs/icon/totop.png");
	background-repeat: no-repeat;
	background-position:0 -50px;	
	width: 48px;
	height:48px;
	cursor: pointer;
	z-index: 9999;
	position: fixed;
	right:30px;
	bottom:59px;
}

/*附加功能：loading...*/
.sr-zql-loading {
	width:100px;
	margin:0 auto;
	padding-top:20px;
	color:gray;
	font-size:16px;
	text-align:center;
}

