INSERT INTO WikiDocContent VALUES ('36', '<h2>提交验证码获得搜索结果</h2><hr><h3 class="page-header">说明</h3><p>贷联盟网站提交验证码后，获得搜索关键字的结果集合。</p><h3 class="page-header">URL</h3><p>http://&lt;Project&gt;/api/dailianmeng/search</p><h3 class="page-header">格式</h3><p>JSON</p><h3 class="page-header">HTTP请求方式</h3><p>GET</p><h3 class="page-header">请求参数</h3><div style="padding-right: 30%;">    <table class="table table-bordered table-hover" style="width:600px;">        <thead>        <tr>            <th>#</th>            <th>必选</th>            <th>类型及范围</th>            <th>说明</th>        </tr>        </thead>        <tbody>        <tr>            <td>isDebug</td>            <td>false</td>            <td>boolean</td>            <td>是否启用调试模式</td>        </tr>        <tr>            <td>serializedFileName</td>            <td>true</td>            <td>String</td>            <td>序列化页面文件名称</td>        </tr>        <tr>            <td>verifycode</td>            <td>true</td>            <td>String</td>            <td>验证码</td>        </tr>        </tbody>    </table></div><h3 class="page-header">注意事项</h3>&nbsp;&nbsp;无<h3 class="page-header">例子</h3><p>说明：非调试模式下调用本接口，获取搜索结果</p><p>请求：http://127.0.0.1:8080/api/dailianmeng/search?serializedFileName=f6ec7c8e-96ed-461e-a1b7-944dedec8eb1&amp;verifycode=5515</p><p>返回：JSON</p><p></p><p style="color:#3da33d;">JSON示例</p><p style="color:#3da33d;">{<br>&nbsp;&nbsp;&nbsp; "data": [<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; {<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; "InfoSource": "成都高新技术产业开发区人民法院",<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; "detailUrl": "/court_blacklist/view/MfQ3Qv.html",<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; "lenderDate": "",<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; "lenderMethod": "",<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; "lenderMoney": "被告于本判决生效之日起十",<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; "name": "王刚",<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; "updateDate": "2015-05-26"<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; },<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; {<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; "InfoSource": "高密市人民法院",<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; "detailUrl": "/court_blacklist/view/J7ZBZz.html",<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; "lenderDate": "",<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; "lenderMethod": "",<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; "lenderMoney": "被告欠原告148323.",<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; "name": "王刚",<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; "updateDate": "2015-05-26"<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; },<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; {<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; "InfoSource": "新疆维吾尔自治区乌苏市人民法院",<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; "detailUrl": "/court_blacklist/view/yimuUn.html",<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; "lenderDate": "",<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; "lenderMethod": "",<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; "lenderMoney": "判决被告杨伟群、王刚应于",<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; "name": "王刚",<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; "updateDate": "2015-05-27"<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; }<br>&nbsp;&nbsp;&nbsp; ]<br>}<br></p><p style="color:#3da33d;"><br></p><p style="color:#3da33d;"><br><br></p><h3 class="page-header" style="color: rgb(0, 0, 0); font-family: Simsun; line-height: normal;">返回字段说明</h3><table class="table table-bordered table-hover" style="width:600px;"><caption>返回正确data结果的字段说明对照表：</caption><thead><tr><th>返回值字段</th><th>字段类型</th><th>字段说明</th></tr></thead><tbody><tr><td><p>name<br></p></td><td>String</td><td><p>姓名<br></p></td></tr><tr><td><p>lenderMoney<br></p></td><td>String</td><td><p>金额<br></p></td></tr><tr><td><p>lenderDate<br></p></td><td>String</td><td><p>借款日期<br></p></td></tr><tr><td><p>lenderMethod<br></p></td><td>String</td><td><p>贷款方式<br></p></td></tr><tr><td><p>InfoSource<br></p></td><td>String</td><td><p>信息来源</p></td></tr><tr><td><p>updateDate<br></p></td><td>String</td><td><p>更新时间<br></p></td></tr><tr><td><p>detailUrl<br></p></td><td>String</td><td><p>详情url<br></p></td></tr></tbody></table>', '36');
INSERT INTO WikiDocContent VALUES ('37', '<h2 style="font-family: \''Helvetica Neue\'', Helvetica, Arial, sans-serif; color: rgb(0, 0, 0);">获取贸易登记查询页</h2><hr><h3 class="page-header" style="font-family: \''Helvetica Neue\'', Helvetica, Arial, sans-serif; color: rgb(0, 0, 0);">说明</h3><p>获取贸易登记查询页的cookie，以及进行查询页的图片验证码。</p><h3 class="page-header" style="font-family: \''Helvetica Neue\'', Helvetica, Arial, sans-serif; color: rgb(0, 0, 0);">URL</h3><p>http://&lt;Project&gt;/api/iecms/querypage</p><h3 class="page-header" style="font-family: \''Helvetica Neue\'', Helvetica, Arial, sans-serif; color: rgb(0, 0, 0);">格式</h3><p>JSON</p><h3 class="page-header" style="font-family: \''Helvetica Neue\'', Helvetica, Arial, sans-serif; color: rgb(0, 0, 0);">HTTP请求方式</h3><p>GET</p><h3 class="page-header" style="font-family: \''Helvetica Neue\'', Helvetica, Arial, sans-serif; color: rgb(0, 0, 0);">请求参数</h3><div style="padding-right: 262.875px;"><table class="table table-bordered table-hover" style="width: 600px;"><thead><tr><th style="line-height: 1.42857;">#</th><th style="line-height: 1.42857;">必选</th><th style="line-height: 1.42857;">类型及范围</th><th style="line-height: 1.42857;">说明</th></tr></thead><tbody><tr><td style="line-height: 1.42857;"><span style="line-height: 20px;">isDebug</span><br></td><td style="line-height: 1.42857;">true</td><td style="line-height: 1.42857;"><span style="line-height: 20px;">boolean</span></td><td style="line-height: 1.42857;"><span style="line-height: 20px;">是否启用调试模式</span><br></td></tr></tbody></table><h3 class="page-header">注意事项</h3><p>无</p></div><h3 class="page-header" style="font-family: \''Helvetica Neue\'', Helvetica, Arial, sans-serif; color: rgb(0, 0, 0);">例子</h3><p>说明：非调试模式下调用本接口，取得查询的cookie及序列化名称信息</p><p>请求：http://54.223.194.5:8080/api/iecms/querypage</p><p>返回：JSON</p><p></p><p style="color: rgb(61, 163, 61);">JSON示例</p><pre class="prettyprint sr-zql-pre-without-border" style="line-height: 1.42857; color: rgb(61, 163, 61);">{    "data": {     