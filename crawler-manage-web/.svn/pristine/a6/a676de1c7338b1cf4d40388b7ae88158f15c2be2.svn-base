INSERT INTO WikiDocContent VALUES ('38', '<h2 style="color: rgb(0, 0, 0); font-family: Simsun; line-height: normal;">执行案件信息</h2><h3 class="page-header" style="color: rgb(0, 0, 0); font-family: Simsun; line-height: normal;">说明</h3><p style="font-family: Simsun; font-size: medium; line-height: normal;">获取执行案件信息。</p><h3 class="page-header" style="color: rgb(0, 0, 0); font-family: Simsun; line-height: normal;">URL</h3><p style="font-family: Simsun; font-size: medium; line-height: normal;">http://&lt;Project&gt;/api/zjsfgkw/zjsfgkwExecuteCaseSearch</p><h3 class="page-header" style="color: rgb(0, 0, 0); font-family: Simsun; line-height: normal;">格式</h3><p style="font-family: Simsun; font-size: medium; line-height: normal;">JSON</p><h3 class="page-header" style="color: rgb(0, 0, 0); font-family: Simsun; line-height: normal;">HTTP请求方式</h3><p style="font-family: Simsun; font-size: medium; line-height: normal;">GET</p><h3 class="page-header" style="color: rgb(0, 0, 0); font-family: Simsun; line-height: normal;">请求参数</h3><table class="table table-bordered"><tbody><tr><td><p>#<br></p></td><td><p>必选<br></p></td><td><p>类型及范围<br></p></td><td><p>说明<br></p></td></tr><tr><td><p>dSR<br></p></td><td><p>true<br></p></td><td>String<br></td><td>案号（哪年）<br></td></tr><tr><td><p>aH_BH<br></p></td><td><p>true<br></p></td><td><p>String<br></p></td><td><p>案号<br></p></td></tr><tr><td><p>aH_NH<br></p></td><td><p>true<br></p></td><td><p>String<br></p></td><td>当事人<br></td></tr><tr><td><p>isDebug<br></p></td><td><p>false<br></p></td><td><p>boolean<br></p></td><td><p>是否启用调试模式<br></p></td></tr></tbody></table><h3 class="page-header" style="color: rgb(0, 0, 0); font-family: Simsun; line-height: normal;">注意事项</h3><p style="font-family: Simsun; font-size: medium; line-height: normal;">无</p><h3 class="page-header" style="color: rgb(0, 0, 0); font-family: Simsun; line-height: normal;">例子</h3><p style="font-family: Simsun; font-size: medium; line-height: normal;">说明：非调试模式下调用本接口，获取执行案件信息。</p><p style="font-fa