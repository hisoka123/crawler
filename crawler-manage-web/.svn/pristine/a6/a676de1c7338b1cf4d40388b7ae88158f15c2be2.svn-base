INSERT INTO WikiDocContent VALUES ('42', '<h2 style="font-family: \''Helvetica Neue\'', Helvetica, Arial, sans-serif; color: rgb(0, 0, 0);">贸易登记查询</h2><hr><h3 class="page-header" style="font-family: \''Helvetica Neue\'', Helvetica, Arial, sans-serif; color: rgb(0, 0, 0);">说明</h3><p>该接口用于贸易登记系统查询。</p><h3 class="page-header" style="font-family: \''Helvetica Neue\'', Helvetica, Arial, sans-serif; color: rgb(0, 0, 0);">URL</h3><p>http://&lt;Project&gt;/api/iecms/getcreditiecms</p><h3 class="page-header" style="font-family: \''Helvetica Neue\'', Helvetica, Arial, sans-serif; color: rgb(0, 0, 0);">格式</h3><p>JSON</p><h3 class="page-header" style="font-family: \''Helvetica Neue\'', Helvetica, Arial, sans-serif; color: rgb(0, 0, 0);">HTTP请求方式</h3><p>POST</p><h3 class="page-header" style="font-family: \''Helvetica Neue\'', Helvetica, Arial, sans-serif; color: rgb(0, 0, 0);">请求参数</h3><div style="padding-right: 262.875px;"><table class="table table-bordered table-hover" style="width: 600px;"><thead><tr><th style="line-height: 1.42857;">#</th><th style="line-height: 1.42857;">必选</th><th style="line-height: 1.42857;">类型及范围</th><th style="line-height: 1.42857;">说明</th></tr></thead><tbody><tr><td style="line-height: 1.42857;">corp_code</td><td><span style="line-height: 20px;">false</span><br></td><td style="line-height: 1.42857;">String</td><td style="line-height: 1.42857;">13位经营者代码</td></tr><tr><td style="line-height: 1.42857;">corp_name</td><td style="line-height: 1.42857;"><span style="line-height: 20px;">false</span><br></td><td style="line-height: 1.42857;">String</td><td style="line-height: 1.42857;">经营者名称</td></tr><tr><td style="line-height: 1.42857;">sc_code</td><td style="line-height: 1.42857;"><span style="line-height: 20px;">false</span><br></td><td style="line-height: 1.42857;">String</td><td style="line-height: 1.42857;">统一社会信用代码</td></tr><tr><td style="line-height: 1.42857;">codea</td><td style="line-height: 1.42857;">true</td><td style="line-height: 1.42857;">String<