INSERT INTO WikiDocContent VALUES ('39', '<h2>获取详情信息</h2><hr><h3 class="page-header">说明</h3><p>获得搜取结果中某条记录的详情信息<br></p><h3 class="page-header">URL</h3><p>http://&lt;Project&gt;/api/dailianmeng/getDetailPage</p><h3 class="page-header">格式</h3><p>JSON</p><h3 class="page-header">HTTP请求方式</h3><p>GET</p><h3 class="page-header">请求参数</h3><div style="padding-right: 30%;">    <table class="table table-bordered table-hover" style="width:600px;">        <thead>        <tr>            <th>#</th>            <th>必选</th>            <th>类型及范围</th>            <th>说明</th>        </tr>        </thead>        <tbody>        <tr>            <td>isDebug</td>            <td>false</td>            <td>boolean</td>            <td>是否启用调试模式</td>        </tr>        <tr>            <td>detailUrl</td>            <td>true</td>            <td>String</td>            <td>获取详情信息的请求路径</td>        </tr>        </tbody>    </table></div><h3 class="page-header">注意事项</h3>&nbsp;&nbsp;无<h3 class="page-header">例子</h3><p>说明：非调试模式下调用本接口，获取详情信息结果</p><p>请求：http://127.0.0.1:8080/api/dailianmeng/getDetailPage?detailUrl=http%3A%2F%2Fwww.dailianmeng.com%2Fcourt_blacklist%2Fview%2FyimuUn.html</p><p>返回：JSON</p><p></p><p style="color:#3da33d;">JSON示例</p><p style="color:#3da33d;">{<br>&nbsp;&nbsp;&nbsp; "data": {<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; "info1": "王刚",<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; "info2": "(2015)乌执字第00199号",<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; "info3": "38",<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; "info4": "男",<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; "info5": "6542221977****151X",<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; "info6": "",<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; "info7": "新疆维吾尔自治区乌苏市人民法院",<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; "info8": "新疆",<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; "info9": "2014乌民一初字第1303号",<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; "info10": "2015年01月27日",<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; "info11": "乌苏市人民法院民事审判庭",<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; "info12": "判决被告杨伟群、王刚应于本判决书生效后十日内支付原告张玉琼支付货款96170元；驳回原告的诉讼请求；案件受理费2204元，减半收取1102元，投递费96元，合计1198元，由被告王刚负担。",<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; "info13": "全部未履行",<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; "info14": "",<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; "info15": "",<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; "info16": "其他有履行能力而拒不履行生效法律文书确定义务",<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; "info17": "2015年05月25日",<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; "info18": "2015-05-27"<br>&nbsp;&nbsp;&nbsp; }<br>}<br></p><p style="color:#3da33d;"><br></p><p style="color:#3da33d;"><br><br></p><h3 class="page-header" style="color: rgb(0, 0, 0); font-family: Simsun; line-height: normal;">返回字段说明</h3><table class="table table-bordered table-hover" style="width:600px;"><caption>返回正确data结果的字段说明对照表：</caption><thead><tr><th>返回值字段</th><th>字段类型</th><th>字段说明</th></tr></thead><tbody><tr><td><p>info1<br></p></td><td>String</td><td><p>被执行人姓名/名称<br></p></td></tr><tr><td><p>info2<br></p></td><td>String</td><td><p>案号<br></p></td></tr><tr><td><p>info3<br></p></td><td>String</td><td><p>年龄<br></p></td></tr><tr><td><p>info4<br></p></td><td>String</td><td><p>性别<br></p></td></tr><tr><td><p>info5<br></p></td><td>String</td><td><p>身份证号/组织机构代码</p></td></tr><tr><td><p>info6<br></p></td><td>String</td><td><p>法定代表人或者负责人<br></p></td></tr><tr><td><p>info7<br></p></td><td>String</td><td><p>执行法院<br></p></td></tr><tr><td><p>info8<br></p></td><td>String</td><td><p>省份<br></p></td></tr><tr><td><p>info9<br></p></td><td>String</td><td><p>执行依据文号<br></p></td></tr><tr><td><p>info10<br></p></td><td>String</td><td><p>立案时间<br></p></td></tr><tr><td><p>info11<br></p></td><td>String</td><td><p>做出执行依据单位<br></p></td></tr><tr><td><p>info12<br></p></td><td>String</td><td><p>生效法律文书确定的义务<br></p></td></tr><tr><td><p>info13<br></p></td><td>String</td><td><p>被执行人的履行情况<br></p></td></tr><tr><td><p>info14<br></p></td><td>String</td><