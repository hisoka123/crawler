INSERT INTO WikiDocContent VALUES ('1', '<h2>搜索用户</h2><hr><h3 class="page-header">说明</h3><p>用户搜索接口是通过微博昵称来查询微博用户的接口。</p><h3 class="page-header">URL</h3><p>http://&lt;Project&gt;/api/user?q=</p><h3 class="page-header">格式</h3><p>JSON</p><h3 class="page-header">HTTP请求方式</h3><p>GET</p><h3 class="page-header">请求参数</h3><div style="padding-right: 30%;">		<table class="table table-bordered table-hover">			<thead>				<tr>					<th>#</th>					<th>必选</th>					<th>类型及范围</th>					<th>说明</th>				</tr>			</thead>			<tbody>				<tr>					<td>q</td>					<td>true</td>					<td>string</td>					<td>查询条件(需要对查询条件进行一次utf8编码)</td>				</tr>                <tr>					<td>isDebug</td>					<td>false</td>					<td>boolean</td>					<td>是否启用调试模式</td>				</tr>			</tbody>		</table>		</div><h3 class="page-header">注意事项</h3><p>无</p><h3 class="page-header">例子</h3><p>说明：非调试模式下，查询微博昵称为“唐沐thomas”的用户</p><p>请求：http://&lt;Project&gt;/api/weibo/user?q=%E5%94%90%E6%B2%90thomas</p><p>返回：JSON</p><p style="color:#3da33d;">JSON示例</p><pre style="color:#3da33d;font-family:宋体">     {    "data": [        {            "uid": "1656627870",            "nickname": "唐沐thomas",            "follow": "426",            "follow_url": "http://weibo.com/1656627870/follow",            "fans": "8万",            "fans_url": "http://weibo.com/1656627870/fans",            "statuses": "1292",            "statuses_url": "http://weibo.com/1656627870/profile",            "profile": "http://weibo.com/thomastang",            "person_info": "简介： 小米路由器产品负责人。2001年入行设计，2003组建腾讯用户研究与体验设计中心（CDC），2013加入小米。",            "person_card": "小米路由器 总经理",            "profile_image": "http://tp3.sinaimg.cn/1656627870/180/5726047174/1",            "location": "北京，海淀区",            "v": "微博个人认证 ",            "gender": "男",            "person_label": [                "电影",                "万宝路",                "摄影",                "用户体验",                "智能硬件",                "智能家居",                "路由器",                "设计"            ],            "person_job": [                "腾讯",                "金山",                "小米科技"            ],            "person_edu": [                "华南理工大学"            ]        }    ]}</pre><h3 class="page-header">返回字段说明</h3><p style="color:#3da33d;">		</p><div style="padding-right: 30%;">		<table class="table table-bordered table-hover">			<caption>返回正确data结果的字段说明对照表：</caption>			<thead>				<tr>					<th>返回值字段</th>					<th>字段类型</th>					<th>字段说明</th>				</tr>			</thead>			<tbody>				<tr>					<td>uid</td>					<td>string</td>					<td>用户的ID</td>				</tr>				<tr>					<td>nickname</td>					<td>string</td>					<td>用户的昵称</td>				</tr>				<tr>					<td>follow</td>					<td>string</td>					<td>用户关注的人数</td>				</tr>				<tr>					<td>follow_url</td>					<td>string</td>					<td>用户关注的人的列表页面</td>				</tr>                <tr>                    <td>fans</td>                    <td>string</td>                    <td>粉丝数量</td>                </tr>                <tr>                    <td>fans_url</td>                    <td>string</td>                    <td>粉丝列表页面</td>                </tr>				<tr>					<td>statuses</td>					<td>string</td>					<td>用户发帖总数</td>				</tr>				<tr>					<td>statuses_url</td>					<td>string</td>					<td>用户发帖的列表页</td>				</tr>				<tr>					<td>profile</td>					<td>string</td>					<td>用户首页</td>				</tr>				<tr>					<td>person_info</td>					<td>string</td>					<td>用户简介</td>				</tr>				<tr>					<td>profile_image</td>					<td>string</td>					<td>用户头像图片链接</td>				</tr>				<tr>					<td>location</td>					<td>string</td>					<td>用户所在地</td>				</tr>				<tr>					<td>gender</td>					<td>string</td>					<td>性别</td>				</tr>				<tr>					<td>person_label</td>					<td>string[]</td>					<td>用户标签</td>				</tr>				<tr>					<td>person_job</td>					<td>string[]</td>					<td>用户的工作经历标签</td>				</tr>				<tr>					<td>person_edu</td>					<td>string[]</td>					<td>用户的教育经历标签</td></tr></tbody></table></div>', '1');
INSERT INTO WikiDocContent VALUES ('2', '<h2>查询用户信息</h2><hr><h3 class="page-header">说明</h3><p>查询用户详细信息接口是通过用户微博info页面来查询微博用户的详细信息的接口。</p><h3 class="page-header">URL</h3><p>http://&lt;Project&gt;/api/userinfo?url=</p><h3 class="page-header">格式</h3><p>JSON</p><h3 class="page-header">HTTP请求方式</h3><p>GET</p><h3 class="page-header">请求参数</h3><div style="padding-right: 30%;">    <table class="table table-bordered table-hover">        <thead>        <tr>            <th>#</th>            <th>必选</th>            <th>类型及范围</th>            <th>说明</th>        </tr>        </thead>        <tbody>        <tr>            <td>url</td>            <td>true</td>            <td>string</td>            <td>用户info页面的URL</td>        </tr>        <tr>            <td>isDebug</td>            <td>false</td>            <td>boolean</td>            <td>是否启用调试模式</td>        </tr>        </tbody>    </table></div><h3 class="page-header">注意事项</h3><p>无</p><h3 class="page-header">例子</h3><p>说明：非调试模式下，查询微博主页地址为“http://weibo.com/1656627870/info”的用户信息。使用“http://weibo.com/thomastang/info”也可以得到同样的搜索结果</p><p>请求：http://&lt;Project&gt;/api/weibo/userinfo?url=http://weibo.com/1656627870/info</p><p>返回：JSON</p><p style="color:#3da33d;">JSON示例</p><pre style="color:#3da33d;font-family:宋体">{    "data": {        "nickname": "唐沐thomas",        "follow": "426",        "fans": "88656",        "statuses": "1292",        "profile": "http://weibo.com/thomastang",        "person_info": "小米路由器产品负责人。2001年入行设计，2003组建腾讯用户研究与体验设计中心（CDC），2013加入小米。",        "location": "北京 海淀区",        "gender": "男",        "birthday": "1977年11月29日",        "created_at": "2009-10-27",        "jobinfo": [            {                "organization": "小米科技",                "organization_url": "http://s.weibo.com/user/&work=%E5%B0%8F%E7%B1%B3%E7%A7%91%E6%8A%80&from=inf&wvr=5&loc=infjob",                "location": "北京 ，海淀区",                "position": "小米路由器产品负责人",                "duration": "2013 - 2013"            },            {                "organization": "腾讯",                "organization_url": "http://s.weibo.com/user/&work=%E8%85%BE%E8%AE%AF&from=inf&wvr=5&loc=infjob",                "location": "广东 ，深圳",                "position": "CDC",                "duration": "2003 - 2013"            },            {                "organization": "金山",                "organization_url": "http://s.weibo.com/user/&work=%E9%87%91%E5%B1%B1&from=inf&wvr=5&loc=infjob",                "location": "广东 ，珠海",                "position": "UI组",                "duration": "2001 - 2003"            }        ],        "educationinfos": [            {                "flag": "大学",                "school": [                    {                        "name": "华南理工大学",                        "url": "http://s.weibo.com/user/&school=%E5%8D%8E%E5%8D%97%E7%90%86%E5%B7%A5%E5%A4%A7%E5%AD%A6&from=inf&wvr=5&loc=infedu"                    }                ]            }        ]    }}</pre><h3 class="page-header">返回字段说明</h3><p style="color:#3da33d;"></p><div style="padding-right: 30%;">    <table class="table table-bordered table-hover">        <caption>返回正确data结果的字段说明对照表：</caption>        <thead>        <tr>            <th>返回值字段</th>            <th>字段类型</th>            <th>字段说明</th>        </tr>        </thead>        <tbody>        <tr>            <td>nickname</td>            <td>string</td>            <td>用户的昵称</td>        </tr>        <tr>            <td>follow</td>            <td>string</td>            <td>用户关注的人数</td>        </tr>        <tr>            <td>fans</td>            <td>string</td>            <td>用户的粉丝人数</td>        </tr>        <tr>            <td>statuses</td>            <td>string</td>            <td>用户发帖总数</td>        </tr>        <tr>            <td>profile</td>            <td>string</td>            <td>用户首页</td>        </tr>        <tr>            <td>person_info</td>            <td>string</td>            <td>用户简介</td>        </tr>        <tr>            <td>location</td>            <td>string</td>            <td>用户所在地</td>        </tr>        <tr>            <td>gender</td>            <td>string</td>            <td>用户的性别</td>        </tr>        <tr>            <td>birthday</td>            <td>string</td>            <td>用户的生日</td>        </tr>        <tr>            <td>created_at</td>            <td>string</td>            <td>用户微博帐号创建时间</td>        </tr>        <tr>            <td>jobinfo</td>            <td>string[]</td>            <td>用户工作经历</td>        </tr>        <tr>            <td>organization</td>            <td>string</td>            <td>工作单位</td>        </tr>        <tr>            <td>organization_url</td>            <td>string</td>            <td>工作单位</td>        </tr>        <tr>            <td>location</td>            <td>string</td>            <td>工作单位地址</td>        </tr>        <tr>            <td>position</td>            <td>string</td>            <td>职位</td>        </tr>        <tr>            <td>duration</td>            <td>string</td>            <td>工作时间段</td>        </tr>        <tr>            <td>educationinfos</td>            <td>string</td>            <td>工作教育经历</td>        </tr>        <tr>            <td>flag</td>            <td>string</td>            <td>教育阶段eg：大学 中学 小学</td>        </tr>        <tr>            <td>school</td>            <td>string</td>            <td>学校</td>        </tr>        <tr>            <td>name</td>            <td>string</td>            <td>学校名字</td>        </tr>        <tr>            <td>url</td>            <td>string</td>            <td>学校的微博连接</td></tr></tbody></table></div>', '2');
INSERT INTO WikiDocContent VALUES ('3', '<h2>查询用户人脉</h2><hr><h3>说明</h3><p>查询用户人脉接口是通过微博id来查询微博用户的人脉信息的接口。</p><h3 class="page-header">URL</h3><p>http://&lt;Project&gt;/api/weibo/friendcircle?uid =&amp;depth= &amp;limit= </p><h3 class="page-header">格式</h3><p>JSON</p><h3 class="page-header">HTTP请求方式</h3><p>GET</p><h3 class="page-header">请求参数</h3><div style="padding-right: 30%;">    <table class="table table-bordered table-hover">        <thead>        <tr>            <th>#</th>            <th>必选</th>            <th>类型及范围</th>            <th>说明</th>        </tr>        </thead>        <tbody>        <tr>            <td>uid</td>            <td>true</td>            <td>string</td>            <td>微博用户ID</td>        </tr>        <tr>            <td>depth</td>            <td>true</td>            <td>string</td>            <td>爬取用户好友页数（最多5页）</td>        </tr>        <tr>            <td>limit</td>            <td>true</td>            <td>string</td>            <td>展示用户数量（最多8个）</td>        </tr>        <tr>            <td>isDebug</td>            <td>false</td>            <td>boolean</td>            <td>是否启用调试模式</td>        </tr>        </tbody>    </table></div><h3 class="page-header">注意事项</h3><p>无</p><h3 class="page-header">例子</h3><p>说明：非调试模式下，查询id为“1749127163”的微博用户的人脉信息，其中爬取页面数量为5页，展示8条结果</p><p>请求：http://&lt;Project&gt;/api/weibo/friendcircle?uid=1749127163&amp;depth=5&amp;limit=8</p><p>返回：JSON</p><p style="color:#3da33d;">JSON示例</p><pre style="color:#3da33d;font-family:宋体">{    "data": {        "uid": "1656627870",        "friend": [            {                "uid": "1645823934",                "nickname": "李大霄",                "follow": "2165",                "follow_url": "http://weibo.com/1645823934/follow",                "fans": "1635840",                "fans_url": "http://weibo.com/1645823934/fans?current=fans",                "statuses": "16472",                "statuses_url": "http://weibo.com/u/1645823934",                "profile": "http://weibo.com/u/1645823934"