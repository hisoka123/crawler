.tip {
    visibility:hidden;
    opacity:0;
    position:absolute;
    top:11px;
    right:-224px;
    width:215px;
    padding:7px 15px;
    font-family:'MuseoSans_Thin';
    line-height:150%;
    color:#FFF;
    background-color:#3c948b;
    border-radius:2px;
    -webkit-transition:all .3s ease-out;
    transition:all .3s ease-out
}
.box-form-fields .row:not(.right) .tip {
    top:50px
}
.tip:after {
    content:'';
    position:absolute;
    right:100%;
    height:0;
    width:0;
    top:13px;
    border-style:solid;
    border-color:transparent;
    border-right-color:#3c948b;
    border-width:6px 10px
}
.tip.visible,.tip.error {
    visibility:visible;
    opacity:1
}
.tip.error {
    background-color:#df6c4f
}
.tip.error:after {
    border-right-color:#df6c4f
}
