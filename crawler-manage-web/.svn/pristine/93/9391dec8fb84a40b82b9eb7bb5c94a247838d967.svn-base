    -moz-box-sizing:border-box;
    box-sizing:border-box
}
#toolbar a {
    float:left;
    display:block;
    width:29px;
    height:34px;
    border-right:1px solid #b6b8b7;
    text-indent:-34em;
    background-image:url(../bundles/tvweb/images/bg-img/bg-sprite-wysihtml5.png);
    background-repeat:no-repeat;
    overflow:hidden;
    -webkit-transition:opacity .25s ease-out;
    transition:opacity .25s ease-out
}
#toolbar a:hover {
    opacity:.7
}
#toolbar a.bold {
    background-position:0 0
}
#toolbar a.italic {
    background-position:-30px 0
}
#toolbar a.underline {
    background-position:-60px 0
}
#toolbar a.list {
    background-position:-90px 0
}
#toolbar a.speech {
    float:right;
    border-right:0;
    border-left:1px solid #b6b8b7;
    background-position:-120px 0
}
#recaptcha_area {
    position:relative
}
.box-login .captcha {
    display:none;
    margin-bottom:20px
}
.box-login .captcha .recaptchatable {
    width:470px
}
@media screen and (max-width:767px) {
    .box-form-menu,.box-form-fields {
        clear:both;
        width:100%;
        padding-right:0
    }
    .box-form-menu {
        margin-bottom:23px
    }
    .box-form-fields .row {
        padding:10px 20px
    }
    .box-login .box-form {
        padding:35px
    }
    .box-login .socialconnect a {
        float:none;
        width:100%;
        margin-bottom:10px
    }
}@media screen and (max-width:468px) {
    .box-form-fields .row.left,.box-form-fields .row.right {
        width:100%;
        padding:10px 20px
    }
    .box-form-fields .row.left {
        padding:0 20px
    }
}