package com.crawler.cnca.domain.json;

import java.io.Serializable;

/**
 * 证书信息
 */
public class Certificate implements Serializable {

	/**
	 * 
	 */
	private static final long serialVersionUID = 5652266874550735760L;

	private String status;// 证书状态
	private String number;// 证书编号
	private String basis;// 认证依据
	private String covering;// 认证覆盖的业务范围
	private String endDate;// 证书到期日期
	private String ProductCategory;// 产品类别

	public String getStatus() {
		return status;
	}

	public void setStatus(String status) {
		this.status = status;
	}

	public String getNumber() {
		return number;
	}

	public void setNumber(String number) {
		this.number = number;
	}

	public String getBasis() {
		return basis;
	}

	public void setBasis(String basis) {
		this.basis = basis;
	}

	public String getCovering() {
		return covering;
	}

	public void setCovering(String covering) {
		this.covering = covering;
	}

	public String getEndDate() {
		return endDate;
	}

	public void setEndDate(String endDate) {
		this.endDate = endDate;
	}

	public String getProductCategory() {
		return ProductCategory;
	}

	public void setProductCategory(String productCategory) {
		ProductCategory = productCategory;
	}

	@Override
	public String toString() {
		return "Certificate [status=" + status + ", number=" + number
				+ ", basis=" + basis + ", covering=" + covering + ", endDate="
				+ endDate + ", ProductCategory=" + ProductCategory + "]";
	}

}
