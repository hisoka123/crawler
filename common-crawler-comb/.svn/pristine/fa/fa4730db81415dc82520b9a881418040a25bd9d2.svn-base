package com.crawler.gsxt.domain.json;

import java.io.Serializable;

/**
 * @author kingly
 * @date 2016年3月28日
 * 企业公示信息->行政处罚信息->行政处罚信息
 */
public class EntpubAAdmpunishInfo implements Serializable { //QygsXzcfXzcfInfo
	private static final long serialVersionUID = -2543998702711414627L;
	
	//行政处罚决定书文号
	private String punishRepNum; //xzcfjdsNum;
	//行政处罚内容
	private String punishContent; //xzcfContent;
	//作出行政处罚决定机关名称
	private String deciAuthority; //zcxzcfjdjgName;
	//作出行政处罚决定日期
	private String deciDateTime; //zcxzcfjdDate;
	//违法行为类型
	private String illegalActType; //wfxwType;
	//备注
	private String note;
		
	//公示日期	
	private String showDate; //新加
	//html内容
	private String html;
	public String getPunishRepNum() {
		return punishRepNum;
	}
	public void setPunishRepNum(String punishRepNum) {
		this.punishRepNum = punishRepNum;
	}
	public String getPunishContent() {
		return punishContent;
	}
	public void setPunishContent(String punishContent) {
		this.punishContent = punishContent;
	}
	public String getDeciAuthority() {
		return deciAuthority;
	}
	public void setDeciAuthority(String deciAuthority) {
		this.deciAuthority = deciAuthority;
	}
	public String getDeciDateTime() {
		return deciDateTime;
	}
	public void setDeciDateTime(String deciDateTime) {
		this.deciDateTime = deciDateTime;
	}
	public String getIllegalActType() {
		return illegalActType;
	}
	public void setIllegalActType(String illegalActType) {
		this.illegalActType = illegalActType;
	}
	public String getNote() {
		return note;
	}
	public void setNote(String note) {
		this.note = note;
	}
	
	
	public String getShowDate() {
		return showDate;
	}
	public void setShowDate(String showDate) {
		this.showDate = showDate;
	}
	public String getHtml() {
		return html;
	}
	public void setHtml(String html) {
		this.html = html;
	}
	@Override
	public String toString() {
		return "EntpubAAdmpunishInfo [punishRepNum=" + punishRepNum
				+ ", punishContent=" + punishContent + ", deciAuthority="
				+ deciAuthority + ", deciDateTime=" + deciDateTime
				+ ", illegalActType=" + illegalActType + ", note=" + note
				+ ", html=" + html + "]";
	}

}
