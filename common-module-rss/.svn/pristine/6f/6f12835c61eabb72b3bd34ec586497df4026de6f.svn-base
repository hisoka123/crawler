package com.module.rss.common;

public enum SourceEnum {
	
	GOOGLE("google","https://news.google.com/news/feeds?q="),
	
	BAIDU("baidu","http://news.baidu.com/ns?tn=newsrss&sr=0&cl=2&rn=20&ct=0&word=");
	  
	private String source;
	
	private String rssfeed;
	
	private SourceEnum(String source,String rssfeed){
		 this.source = source; 
		 this.rssfeed = rssfeed;
	}
	 
	public String getSource() { 
        return source; 
    }

	public String getRssfeed() {
		return rssfeed;
	}

	public void setRssfeed(String rssfeed) {
		this.rssfeed = rssfeed;
	}

	public void setSource(String source) {
		this.source = source;
	} 
	
	

}
